<Window x:Class="Battleship.UserWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Battleship"
        mc:Ignorable="d"
        Title="UserWindow" Height="300" Width="300">
    <Grid>
        <TextBox HorizontalAlignment="Left" Height="23" Margin="27,217,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120" TextChanged="TextBox_TextChanged"/>
        <Button x:Name="AddFriend" Content="Add" HorizontalAlignment="Left" Margin="166,220,0,0" VerticalAlignment="Top" Width="75" Click="AddFriend_Click"/>
        <TextBox HorizontalAlignment="Left" Height="23" Margin="104,20,0,0" TextWrapping="Wrap" Text="Friends" VerticalAlignment="Top" Width="64" RenderTransformOrigin="0.154,0.228" FontSize="16" BorderBrush="{x:Null}"/>

    </Grid>
</Window>
